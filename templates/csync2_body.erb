
  key <%= @configpath %>/<%= @group_key %>;
	
<% Array(@includes).sort.each do |incval| -%>
  include <%= incval %>;
<% end -%>

<% if @excludes -%>
<% Array(@excludes).sort.each do |excval| -%>
  exclude <%= excval %>;
<% end -%>
<% end -%>

  #Setting for automatic file dispute resolution
  auto <%= @auto %>;

}

